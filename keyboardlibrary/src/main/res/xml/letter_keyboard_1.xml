    <Row android:verticalGap="1%p">

        <Key
            android:codes="110"
            android:keyEdgeFlags="left"
             android:keyLabel="N" />
        <Key
            android:codes="112"
            android:horizontalGap="2%p"
            android:keyEdgeFlags="left"
             android:keyLabel="P" />

        <Key
            android:codes="113"
            android:horizontalGap="2%p"
            android:keyEdgeFlags="left"
             android:keyLabel="Q" />
        <Key
            android:codes="114"
            android:horizontalGap="2%p"
            android:keyEdgeFlags="left"
             android:keyLabel="R" />


        <Key
            android:codes="-4"
            android:horizontalGap="2.5%p"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="right"
            android:keyLabel="关闭"/>
    </Row>

</Keyboard>